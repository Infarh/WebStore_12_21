@model IEnumerable<string>
@{
    ViewBag.Title = "Тестовое WebAPI";
}

<div class="container">
    <table class="table">
        <thead>
        <tr>
            <th>#</th>
            <th>Value</th>
        </tr>
        </thead>
        <tbody>
            @{
                var i = 1;
            }
            @foreach (var value in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>@value</td>
                </tr>
            }
        </tbody>
    </table>
</div>